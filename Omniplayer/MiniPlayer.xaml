<Window x:Class="Omniplayer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Omniplayer"
        mc:Ignorable="d"
        Title="MiniPlayer" Height="350" Width="525">
    <Grid Loaded="Grid_Loaded">
        <Button x:Name="button" Content="Play" HorizontalAlignment="Left" Margin="74,138,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Button x:Name="button_Copy" Content="Pause" HorizontalAlignment="Left" Margin="166,138,0,0" VerticalAlignment="Top" Width="75" Click="button_Copy_Click"/>
        <Button x:Name="button_Copy1" Content="Skip" HorizontalAlignment="Left" Margin="262,138,0,0" VerticalAlignment="Top" Width="75" Click="button_Copy1_Click"/>
        <Button x:Name="button_Copy2" Content="Previous" HorizontalAlignment="Left" Margin="357,138,0,0" VerticalAlignment="Top" Width="75" Click="button_Copy2_Click"/>

    </Grid>
</Window>
